{
    "builds": [
        {
            "src": "main_vercel.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main_vercel.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 1 * * 1"
        }
    ]
}